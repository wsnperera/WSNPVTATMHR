            $('table th input:checkbox').on('click', function() {
    var that = this;
            $(this).closest('table').find('tr > td:first-child input:checkbox')
            .each(function() {
            this.checked = that.checked;
                    $(this).closest('tr').toggleClass('selected');
            });
    });
            $('[data-rel="tooltip"]').tooltip({placement: tooltip_placement});
            function tooltip_placement(context, source) {
            var $source = $(source);
                    var $parent = $source.closest('table')
                    var off1 = $parent.offset();
                    var w1 = $parent.width();
                    var off2 = $source.offset();
                    var w2 = $source.width();
                    if (parseInt(off2.left) < parseInt(off1.left) + parseInt(w1 / 2))
                    return 'right';
                    return 'left';
            }


  

 
  

 </script>

 <script type="text/javascript">
     

     $(document).ready(function() {

         $('#CourseBatch').change(function(){

       // alert('dg');
       //e.preventDefault();
       var CDID = document.getElementById('CourseListCode').value; 
       var Year = document.getElementById('Year').value;
       var Batch = document.getElementById('CourseBatch').value; 
        //var a=1;
      
				
				var msg1 = '--- Select Calender Year ---';
        $("#CalenderYear").html('');
				  $.ajax  ({
                     beforeSend: function()
                    {
                        document.getElementById('img77').innerHTML = "<img src=\"<?php echo Url('assets/images/abc.gif'); ?>\"/>";
                    },
                    url: "<?php echo url::to('ViewWeekTimeTableLoadCalenderYear'); ?>",
                    data: { Batch: Batch,Year: Year,CDID: CDID},
                    dataType: "json", 
                    success: function(result) {

                        //alert(result);
                        $("#CalenderYear").append("<option value=''>" + msg1 + "</option>");
                         $.each(result, function(i, item)
                        {



                            $("#CalenderYear").append("<option value=" + item.Year + ">"+ item.Year+"</option>");
                           // a = a +1;



                        });
                                        
                        
                        },complete: function() {
                                document.getElementById('img77').innerHTML ="";

                            }


                    
                });
        


       
    });
	
