  <script type="text/javascript">
   
                                function getTable() {
                                     var CDID = document.getElementById('CourseListCode').value; 
									   var Year = document.getElementById('Year').value;
									   var Batch = document.getElementById('CourseBatch').value;
										var Month = document.getElementById('Month').value;; 
										var WeekNo = document.getElementById('WeekNo').value;
										 var CalenderYear = document.getElementById('CalenderYear').value;

                                       

                                    //alert(dateRange);

                                 

                                        $.ajax
                                                ({
													beforeSend: function()
													{
														
														document.getElementById('img').innerHTML = "<img src=\"<?php echo Url('assets/images/ajax-loader.gif'); ?>\"/>";
													},
                                                    type: "POST",
                                                    url: "<?php echo Url('loadWeekTimeTableWiuthAjax'); ?>",
                                                    data: {CDID: CDID, Year: Year,Batch:Batch,Month: Month,WeekNo: WeekNo,CalenderYear: CalenderYear},
                                                   // dataType: 'json',
                                                    success: function (result)
                                                    {
														
                                                        
                                                            $('#table').html(''); 
                                                            $('#table').html(result);
                                                        

                                                    },
													complete: function() {
														document.getElementById('img').innerHTML ="";

													}
                                                });
                                    
                                }
 </script>

  <script type="text/javascript">
     

     $(document).ready(function() {

        $('#upload').click(function()
    {
      //alert('in upload');
      //alert('Do You wanna to Ptint this Time Table');
		var CD_ID = document.getElementById('CourseListCode').value; 
		var Year = document.getElementById('Year').value;
		var Batch = document.getElementById('CourseBatch').value;
		var MonthID = document.getElementById('Month').value;; 
		var WeekNo = document.getElementById('WeekNo').value;
		var CalenderYear = document.getElementById('CalenderYear').value;

            $.ajax
                    ({
                       
                        type: "POST",
                        url: "<?php echo Url('PrintPDFWeekTimeTableLoadWeekNo'); ?>",
                        data: {CD_ID: CD_ID,Year: Year,Batch: Batch,WeekNo: WeekNo,MonthID:MonthID,CalenderYear:CalenderYear},
                        success:function response(responseText, statusText, xhr, $form)
                        {
       
                               var printWin = window.open("","printSpecial");
                                printWin.document.open();
                                printWin.document.write(responseText);
                                printWin.document.close();
                                printWin.print();
       
   
                         

                        }
                    });
        
    }); 

