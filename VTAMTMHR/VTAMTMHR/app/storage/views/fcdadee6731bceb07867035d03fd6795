	   var All = 'All';
        $("#WeekNo").html('');
       $.ajax  ({
                    beforeSend: function()
                    {
                        document.getElementById('img6').innerHTML = "<img src=\"<?php echo Url('assets/images/abc.gif'); ?>\"/>";
